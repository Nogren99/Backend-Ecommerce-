<div>
    <h1>Listado de productos:</h1>
</div>

<div>
    {{#each products}}
        <div>
            <h2>{{title}}</h2>
            <p>Descripcion:{{this.description}}</p>
            <p>code: {{this.code}}</p>
            <p>Estado: {{this.stats}}</p>
            <p>Stock:{{this.stock}}</p>
            <p>Price:{{this.price}}</p>
            <p>Id:{{this.id}}</p>
        </div>
    {{/each}}
</div>



